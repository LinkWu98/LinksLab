        "/swagger/updatePreSaleOrder": {
            "post": {
                "tags": [
                    "swagger-controller"
                ],
                "summary": "预售订单，C端支付大定后，小转大完善订单信息",
                "operationId": "updatePreSaleOrderUsingPOST",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "*/*"
                ],
                "parameters": [
                    {
                        "in": "query",
                        "name": "modifyC40OrderDTO",
                        "description": "完善订单信息所需参数",
                        "required": true
                    },
                    {
                        "name": "soNoId",
                        "in": "query",
                        "description": "订单id",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "in": "body",
                        "name": "updatePresaleOrderDTO",
                        "description": "updatePresaleOrderDTO",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/UpdatePresaleOrderDTO"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/AppletDataResponse"
                        }
                    },
                    "201": {
                        "description": "Created"
